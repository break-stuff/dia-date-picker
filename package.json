{
  "name": "ks-date-picker",
  "private": true,
  "version": "0.0.0",
  "main": "dist/my-element.es.js",
  "exports": {
    ".": "./dist/my-element.es.js"
  },
  "types": "types/my-element.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dev": "run-p manifest:dev storybook",
    "serve": "vite --open",
    "build": "npm run clean && vite build && tsc --emitDeclarationOnly && npm run manifest",
    "test": "web-test-runner",
    "test:watch": "web-test-runner --watch",
    "test:unit": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "manifest:dev": "cem analyze --dev --watch --litelement --globs \"./src/**/index.ts\"",
    "manifest": "cem analyze --litelement --globs \"./src/**/index.ts\""
  },
  "dependencies": {
    "lit": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@open-wc/eslint-config": "^7.0.0",
    "@open-wc/testing": "^3.0.3",
    "@storybook/addon-a11y": "^6.4.14",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-controls": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/web-components": "^6.4.14",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@web/dev-server": "^0.1.29",
    "@web/dev-server-esbuild": "^0.2.16",
    "@web/test-runner": "^0.13.26",
    "@web/test-runner-playwright": "^0.8.8",
    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.4.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "lit-html": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "storybook-builder-vite": "^0.1.17",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "customElements": "/custom-elements.json"
}
